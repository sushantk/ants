{
    // A very simple module definition with a single argument "string"
    "module": {
        "@class": "ants.FetcherEcho",
        "string": ""
    }

    // Using the above module in some other module
    "demo": {
        "@class": "example"
        "child": {
            "id": "echo",
            "params": {
                "string": "Hello World"
            }
        }
    }
}